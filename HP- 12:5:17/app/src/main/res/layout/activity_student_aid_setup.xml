<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="victorsorel.bmccbetaversion1.StudentAidSetup">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="S T U D E N T"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="38dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="A I D"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="131dp"
        android:layout_height="26dp"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="207dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date of Birth (mm/dd/yyyy)"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="106dp"
        android:layout_marginTop="33dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Spinner
        android:id="@+id/choose"
        android:layout_width="172dp"
        android:layout_height="0dp"
        android:entries="Black White Asian PacificIslannd"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="331dp" />

    <Spinner
        android:id="@+id/spinner5"
        android:layout_width="172dp"
        android:layout_height="0dp"
        android:entries="Male Female Other"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="381dp" />

    <Spinner
        android:id="@+id/spinner4"
        android:layout_width="172dp"
        android:layout_height="0dp"
        android:entries="Male Female Other"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="277dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gender"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="247dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Race/Ethnicity"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="314dp" />

</android.support.constraint.ConstraintLayout>
